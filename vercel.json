{
  "routes": [
    {
      "handle": "filesystem",
      "headers": {
        "Cache-Control": "max-age=86400"
      }
    },
    {
      "src": "/(.*)",
      "headers": { "Cache-Control": "max-age=86400" },
      "continue": true
    },
    {
      "src": "/(.*)",
      "status": 404, 
      "dest": "/404.html" 
    }
  ]
}
