{
  "routes": [
    { 
      "handle": "filesystem"
    },
    { 
      "src": "/(.*)", 
      "status": 404, 
      "dest": "/404.html" 
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "max-age=604800"
        }
      ]
    }
  ]
}
