{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "headers": { "Cache-Control": "max-age=604800" },
      "continue": true
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.m4s)",
      "headers": { "Content-Type": "video/iso.segment" },
      "continue": true
    },
    { 
      "src": "/(.*)", 
      "status": 404, 
      "dest": "/404.html" 
    }
  ]
}
